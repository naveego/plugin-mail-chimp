{
  "id": "plugin-mail-chimp",
  "displayName": "Mailchimp",
  "description": "Plugin for using Mailchimp to send templated emails.",
  "apiVersion": "2",
  "versionDescription": "Version 2",
  "version": "2.0.0",
  "iconFile": "icon.jpg",
  "executable": "PluginMailChimp",
  "kind": "publisher",
  "os": "$GOOS",
  "arch": "$GOARCH",
  "accessGroups": [],
  "canConfigure": true,
  "canProduceMultipleSchemas": true,
  "canAcceptQueryBasedSchemas": false,
  "canUseOAuth": false,
  "canConfigureWrite": true,
  "canWrite": true,
  "configSchema": {
    "ui": {
      "ui:order": [
        "ApiKey"
      ]
    },
    "schema": {
      "type": "object",
      "properties": {
        "ApiKey" : {
          "type": "string",
          "title": "Api Key",
          "description": "Api Key to use for authentication."
        }
      },
      "required": [
        "ApiKey"
      ]
    }
  }
}